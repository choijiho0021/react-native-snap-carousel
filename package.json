{
  "name": "RokebiESIM",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:android_release": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res;rm -rf ./android/app/src/main/res/drawable-* && rm -rf ./android/app/src/main/res/raw/*.mp4; cd android; ./gradlew clean; cd ..; ",
    "build:android_staging": "react-native bundle --platform android --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --dev false --reset-cache --assets-dest android/app/src/main/res;rm -rf ./android/app/src/main/res/drawable-* && rm -rf ./android/app/src/main/res/raw/*.mp4; cd android; ./gradlew clean; cd ..;",
    "build:android_debugging": "react-native bundle --platform android --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --dev true --reset-cache --assets-dest android/app/src/main/res ;rm -rf ./android/app/src/main/res/drawable-* && rm -rf ./android/app/src/main/res/raw/*.mp4; cd android; ./gradlew clean; cd ..;",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "build_prod": "react-native run-ios --configuration Release",
    "android": "react-native run-android --variant=EsimDebug",
    "android_staging_bundle": "cd android;export ENVFILE=.env.staging && ./gradlew bundle$1;open ./app/build/outputs/bundle/debug$1",
    "ios": "react-native run-ios --simulator 'iPhone 12'",
    "ios_usim": "react-native run-ios --simulator 'iPhone 12' --scheme USIM --configuration USIM.Debug",
    "ios_esim": "react-native run-ios --simulator 'iPhone 12' --scheme RokebiESIM --configuration ESIM.Debug",
    "ios_global": "react-native run-ios --simulator 'iPhone 12' --scheme Global --configuration Global.Debug",
    "build": "echo build start",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android_esim_debugging_apk": "cd android;./gradlew assembleEsimDebug;open ./app/build/outputs/apk/esim/debug",
    "android_esim_staging_apk": "cd android;./gradlew assembleEsimStaging;open ./app/build/outputs/apk/esim/staging",
    "android_esim_prod_apk": "cd android;./gradlew assembleEsimRelease;open ./app/build/outputs/apk/esim/release",
    "android_esim_prod_bundle": "cd android;./gradlew bundleEsimRelease;open ./app/build/outputs/bundle/esimRelease",
    "android_global_staging_apk": "cd android;./gradlew assembleGlobalStaging;open ./app/build/outputs/apk/global/staging",
    "android_global_prod_apk": "cd android;./gradlew assembleGlobalRelease;open ./app/build/outputs/apk/global/release",
    "android_global_prod_bundle": "cd android;./gradlew bundleGlobalRelease;open ./app/build/outputs/bundle/global/release",
    "code_push_release": "sh ./code_push.sh release $1 $2",
    "code_push_patch": "sh ./code_push.sh patch $1 $2",
    "code_push_promote": "sh ./code_push.sh promote production $1",
    "postinstall": "node ./android//fixAndroid/android-release-fix.js",
    "checkLang": "./constants/locale/compare.sh ./constants/locale/ko.json ./constants/locale/en.json"
  },
